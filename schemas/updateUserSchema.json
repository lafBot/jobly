{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "testUserChanged",
            "password": "Password1",
            "first_name": "Updated First",
            "last_name": "Updated Last",
            "email": "newemail@email.com",
            "photo_url": "newurl.jpg",
            "is_admin": "False"
        }
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "The username selected by the user.",
            "default": "",
            "examples": [
                "testUserChanged"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "The user's password.",
            "default": "",
            "examples": [
                "Password1"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "The user's first name.",
            "default": "",
            "examples": [
                "Updated First"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "The user's last name.",
            "default": "",
            "examples": [
                "Updated Last"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "The user's email.",
            "default": "",
            "examples": [
                "newemail@email.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "The url for the user's profile picture.",
            "default": "",
            "examples": [
                "newurl.jpg"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "string",
            "title": "The is_admin schema",
            "description": "Boolean value of user's status as admin.",
            "default": "False",
            "examples": [
                "False"
            ]
        }
    },
    "additionalProperties": true
}