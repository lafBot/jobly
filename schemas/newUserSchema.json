{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "testUser",
            "password": "Password1",
            "first_name": "Leif",
            "last_name": "Flor",
            "email": "email@gmail.com",
            "photo_url": "url.jpg",
            "is_admin": "True"
        }
    ],
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "The user's chosen username.",
            "default": "",
            "examples": [
                "testUser"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "The user's password.",
            "default": "",
            "examples": [
                "Password1"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "The user's first name.",
            "default": "",
            "examples": [
                "Leif"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "The user's last name.",
            "default": "",
            "examples": [
                "Flor"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "The user's provided email.",
            "default": "",
            "examples": [
                "email@gmail.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "Image url source.",
            "default": "",
            "examples": [
                "url.jpg"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "string",
            "title": "The is_admin schema",
            "description": "Administration status of user.",
            "default": "",
            "examples": [
                "True"
            ]
        }
    },
    "additionalProperties": true
}